{
  "name": "@claude-code-web/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@claude-code-web/eslint-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  }
}
